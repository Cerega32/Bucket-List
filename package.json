{
	"name": "bucket-list",
	"version": "0.1.0",
	"private": true,
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": "commitizen.js"
		}
	},
	"dependencies": {
		"@types/node": "^16.18.22",
		"@types/react": "^18.0.31",
		"@types/react-dom": "^18.0.11",
		"@types/react-router-dom": "^5.3.3",
		"mobx-react": "^7.6.0",
		"package-name": "^0.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.10.0",
		"react-svg": "^16.1.18"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"stylelint": "stylelint '**/*{.css,.scss}'",
		"stylelint:fix": "stylelint '**/*{.css,.scss}' --fix",
		"stylelint:report": "stylelint src/**/*.scss --output-file report.txt",
		"pre-commit": "lint-staged",
		"pre-push": "node --max_old_space_size=6144 ./node_modules/.bin/tsc --noEmit --locale ru",
		"prepare": "husky install"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"eslint --fix"
		],
		"src/**/*.scss": [
			"stylelint --config ./stylelint.config-extended.js --fix"
		]
	},
	"devDependencies": {
		"@csstools/postcss-global-data": "^1.0.3",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"@vitejs/plugin-legacy": "^4.0.2",
		"@vitejs/plugin-react": "^3.1.0",
		"cz-customizable": "^7.0.0",
		"eslint": "^8.37.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.2.1",
		"postcss": "^8.4.21",
		"postcss-custom-media": "^9.1.2",
		"postcss-normalize": "^10.0.1",
		"postcss-normalize-charset": "^6.0.0",
		"postcss-preset-env": "^8.3.0",
		"postcss-sort-media-queries": "^4.3.0",
		"prettier": "2.8.7",
		"sass": "^1.61.0",
		"stylelint": "^15.4.0",
		"stylelint-config-recommended-scss": "^9.0.1",
		"stylelint-no-unsupported-browser-features": "^6.1.0",
		"stylelint-order": "^6.0.3",
		"stylelint-prettier": "^3.0.0",
		"typescript": "^5.0.3",
		"vite": "^4.2.1"
	},
	"author": "Sergey Grivin",
	"license": "ISC",
	"description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
	"main": "commitizen.js",
	"keywords": []
}
