{
	"name": "bucket-list",
	"version": "0.1.0",
	"private": true,
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": "commitizen.js"
		}
	},
	"dependencies": {
		"@types/node": "^16.18.22",
		"@types/react": "^18.0.31",
		"@types/react-dom": "^18.0.11",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"scripts": {
		"start": "cross-env SERVE=true webpack serve",
		"build": "webpack",
		"build-prod": "webpack --mode=production",
		"clean": "rd /s /q dist",
		"stylelint": "stylelint '**/*{.css,.scss}'",
		"stylelint:fix": "stylelint '**/*{.css,.scss}' --fix",
		"stylelint:report": "stylelint src/**/*.scss --output-file report.txt",
		"pre-commit": "lint-staged",
		"pre-push": "node --max_old_space_size=6144 ./node_modules/.bin/tsc --noEmit --locale ru",
		"prepare": "husky install"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"eslint --fix"
		],
		"src/**/*.scss": [
			"stylelint --config ./stylelint.config-extended.js --fix"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.21.4",
		"@babel/preset-env": "^7.21.4",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.21.4",
		"@csstools/postcss-global-data": "^1.0.3",
		"@pmmmwh/react-refresh-webpack-plugin": "github:pmmmwh/react-refresh-webpack-plugin",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^9.1.2",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.3",
		"cz-customizable": "^7.0.0",
		"eslint": "^8.37.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"html-loader": "^4.2.0",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.2.1",
		"mini-css-extract-plugin": "^2.7.5",
		"postcss": "^8.4.21",
		"postcss-custom-media": "^9.1.2",
		"postcss-loader": "^7.2.4",
		"postcss-normalize": "^10.0.1",
		"postcss-normalize-charset": "^6.0.0",
		"postcss-preset-env": "^8.3.0",
		"postcss-sort-media-queries": "^4.3.0",
		"prettier": "2.8.7",
		"react-refresh": "^0.14.0",
		"sass": "^1.61.0",
		"sass-loader": "^13.2.2",
		"style-loader": "^3.3.2",
		"stylelint": "^15.4.0",
		"stylelint-config-recommended-scss": "^9.0.1",
		"stylelint-no-unsupported-browser-features": "^6.1.0",
		"stylelint-order": "^6.0.3",
		"stylelint-prettier": "^3.0.0",
		"typescript": "^5.0.3",
		"webpack": "^5.78.0",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^4.13.2"
	},
	"author": "Sergey Grivin",
	"license": "ISC",
	"description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
	"main": "commitizen.js",
	"keywords": []
}
